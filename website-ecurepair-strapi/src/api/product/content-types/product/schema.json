{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "onderdeelnummer": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "samenvatting": {
      "type": "text",
      "required": true
    },
    "omschrijving": {
      "type": "richtext",
      "required": true
    },
    "onderdeel": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::onderdeel.onderdeel",
      "inversedBy": "products"
    },
    "afbeelding": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "merks": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::merk.merk",
      "inversedBy": "products"
    }
  }
}
